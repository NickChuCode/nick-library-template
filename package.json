{
  "name": "nick-library-template",
  "version": "1.0.0",
  "description": "a template for building an open source library",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc mocha",
    "lint": "eslint src",
    "watch:test": "mocha --watch",
    "validateWithoutNpmRunAll": "npm run lint && npm run test",
    "validate": "npm-run-all --parallel lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickChuCode/nick-library-template.git"
  },
  "keywords": [
    "template"
  ],
  "author": "Nick Chu <nickchu15@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NickChuCode/nick-library-template/issues"
  },
  "homepage": "https://github.com/NickChuCode/nick-library-template#readme",
  "dependencies": {
    "unique-random-array": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-kentcdodds": "^9.0.0",
    "ghooks": "^2.0.4",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0"
  },
  "nyc": {
    "check-coverage": true,
    "functions": 100,
    "lines": 100,
    "branches": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
